<?xml version="1.0" encoding="UTF-8"?>
<ae:configurations
    xmlns="http://berlinonline.de/schemas/honeybee/config/navigation/1.0"
    xmlns:env="http://berlinonline.de/schemas/honeybee/config/envelope/definition/1.0"
    xmlns:ae="http://agavi.org/agavi/config/global/envelope/1.0"
>
    <ae:configuration>

        <available_items>
            <items name="{{ vendor_prefix }}.{{ package_prefix }}">
            </items>
        </available_items>

        <navigations default="main">
            <navigation name="main">
                <groups>
                    <group name="{{ vendor_prefix }}.{{ package_prefix }}">
                        <!--
                            It is possible to specify a 'translation_domain' setting
                            for translating all the items of the navigation group.

                            <settings>
                                <setting name="translation_domain">application.activities</setting>
                            </settings>
                        -->
                        <include>{{ vendor_prefix }}.{{ package_prefix }}</include>
                    </group>
                </groups>
            </navigation>
        </navigations>

    </ae:configuration>
</ae:configurations>

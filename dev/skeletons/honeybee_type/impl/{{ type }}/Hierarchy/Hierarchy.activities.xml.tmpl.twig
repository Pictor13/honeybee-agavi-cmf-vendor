<?xml version="1.0" encoding="UTF-8"?>
<ae:configurations
    xmlns="http://berlinonline.de/schemas/honeybee/config/activities/1.0"
    xmlns:ae="http://agavi.org/agavi/config/global/envelope/1.0"
>
    <ae:configuration>
        <activity_containers>
            <activity_container scope="{{ vendor_prefix }}.{{ package_prefix }}.{{ type_prefix }}.hierarchy" extends="{{ vendor_prefix }}.{{ package_prefix }}.{{ type_prefix }}.collection">
            </activity_container>

            <activity_container scope="{{ vendor_prefix }}.{{ package_prefix }}.{{ type_prefix }}.hierarchy.primary_activities" extends="{{ vendor_prefix }}.{{ package_prefix }}.{{ type_prefix }}.collection.primary_activities">
            </activity_container>

            <activity_container scope="{{ vendor_prefix }}.{{ package_prefix }}.{{ type_prefix }}.hierarchy.sort_activities" extends="{{ vendor_prefix }}.{{ package_prefix }}.{{ type_prefix }}.collection.sort_activities">
            </activity_container>
        </activity_containers>
    </ae:configuration>
</ae:configurations>

- extends _layout

- block primary
    <h2>Resource/WorkflowError</h2>
    - embed 'html/layout/grid/1_boxes.twig' with {'name':'workflow', 'box1':'content'}
        - block box1
            %p An unexpected error occured while trying to handle workflow request.
            - if errors is defined
                %ul.errors
                    - for error in errors
                        %li.error
                            %span = error

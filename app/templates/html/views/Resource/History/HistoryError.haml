- extends _template

- block primary
    <h2>Item/HistoryError</h2>
    - embed 'html/layout/grid/1_boxes.twig' with {'name':'event_stream', 'box1':'login'}
        - block box1
            %p An unexpected error occured while trying to load/process event stream.
            - if errors is defined
                %ul.errors
                    - for error in errors
                        %li.error
                            %span = error
